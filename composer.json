{
  "name": "tyler-sommer/nice-bench",
  "description": "A nice PHP benchmarking framework",
  "license": "MIT",
  "require": {
    "php": ">=5.3.3"
  },
  "require-dev": {
    "phpunit/phpunit": "~3.7"
  },
  "autoload": {
    "psr-4": {
      "Nice\\Tests\\Benchmark\\": "tests/",
      "Nice\\Benchmark\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "minimum-stability": "beta",
  "prefer-stable": true
}
